<% layout('layout') -%>



<form method="POST">
      <h1>Sign Up</h1>
      <div class="form-group">
          <label="category">category:</label>
          <input type="text" class="form-control" name="category">
      </div>
      <div class="form-group">
          <label="name">Name:</label>
          <input type="text" class="form-control" name="name">
      </div>
      <div class="form-group">
        <label="image">image:</label>
          <input type="text" class="form-control" name="image">
      </div>
      <div class="form-group">
        <label="class">type:</label>
        <input type="text" class="form-control" name="class">
      </div>
      <div class="form-group">
        <label="state">State:</label>
        <input type="text" class="form-control" name="state">
      </div>
      <div class="form-group">
        <label="website">website:</label>
        <input type="text" class="form-control" name="website">
      </div>
      <div class="form-group">
        <label="fees">fees:</label>
        <input type="Number" class="form-control" name="fees">
      </div>
      <div class="form-group">
        <label="phonenumber">Phone Number:</label>
        <input type="Number" class="form-control" name="phonenumber">
      </div>
      <div class="form-group">
        <label="description">Description:</label>
        <textarea class="form-control" id="addProduct" name="description"></textarea>
      </div>
     
      <button type="submit" class="btn btn-success">submit</button>
    </form>