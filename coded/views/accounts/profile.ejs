<% layout('layout') -%>
<div class="container">
  <div class="row>
  <div class="col-sm-6 col-md-4">
    <img class="img-circle img-responsive img-center" src="<%=user.profile.picture %> alt="">

    <br />
    <p>First Name: <%= user.profile.firstname %></p>
    <p>Last Name: <%= user.profile.lastname %></p>
    <p>Email: <%= user.email %></p>
    <p>Phone No: <%= user.phonenumber %></p>
    <p>Coded Type: <%= user.profile.coded %></p>
    <p>Address: <%= user.address %></p>

    <a href="/edit-profile" class="btn btn-primary btn-lg">Edit Profile</a>
  </div>
  </div>


</div>
